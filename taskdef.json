{
  "executionRoleArn": "arn:aws:iam::902558168644:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 5000,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "name": "bytecloud",
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::bytecloud-env/.env",
          "type": "s3"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024",
  "family": "task-bytecloud"
}
